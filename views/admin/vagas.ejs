<%- include('header') %>
    <h2>
        <div id="admin_vagas"> Admin de Vagas </div>
    </h2>
    <h2>
          <div id="nova_vaga"><a href="/admin/vagas/nova">Nova vaga</a></div>
    </h2>
    
    <div>
        <ul>
            <% vagas.forEach(vaga => { %>
            <li>
                <%- vaga.titulo %> |
                <a href="/admin/vagas/delete/<%- vaga.id%>">Excluir</a>
                <a href="/admin/vagas/editar/<%- vaga.id%>">Editar</a>
            </li>
            <% }) %>
        </ul>
    </div>
<%- include('footer') %>