<!DOCTYPE html>
<html lang="pt-br">

<head>
  <meta charset="UTF-8" />
  <link href="https://fonts.googleapis.com/css?family=Roboto&display=swap" rel="stylesheet" />

  <link href="/styles/main.css" rel="stylesheet" />
  <link href="/styles/pagecategorys.css" rel="stylesheet" />
  <link
    href="https://fonts.googleapis.com/css2?family=Archivo:wght@400;700&amp;family=Poppins:wght@400;600&amp;display=swap"
    rel="stylesheet" />

  <title>Jobify</title>
</head>

<body id="page-category">
  <div class="page-header">
    <a href="/admin">
      <img src="/images/back.svg" alt="Back" srcset="" />
    </a>
    <div class="page-title">
      <h1>
        Manage categorys
      </h1>
    </div>

  </div>

  <div id="page-categorys">
    <div class="categorys-list">
      <ul>
        <% categorias.forEach(categoria => { %>

        <li>
          <%- categoria.categoria %>

          <a href="/admin/categorias/editar/<%- categoria.id%>">
            <img src="/images/edit.svg" alt="edit category" srcset="">
          </a>

          <a href="/admin/categorias/delete/<%- categoria.id%>">
            <img src="/images/delete.svg" alt="delete category" srcset="">
          </a>
        </li>

        <% }) %>
      </ul>
    </div>

    <div class="buttons-container">
      <a href="/admin/categorias/nova">New category</a>
    </div>
  </div>
</body>

</html>